package model.bean;

import java.util.Calendar;

public class Exame {

	private int id;
	private int id_pcnt;
	private String nm_exame;
	private Calendar exm_data;
	private String resultado;
	private Paciente Exame_paciente;

	public Exame() {
		super();
	}

	public Exame(int id, int id_pcnt, String nm_exame, Calendar exm_data ,String resultado) {
		super();
		this.id = id;
		this.id_pcnt = id_pcnt;
		this.nm_exame = nm_exame;
		this.exm_data = exm_data;
		this.resultado = resultado;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getId_pcnt() {
		return id_pcnt;
	}

	public void setId_pcnt(int id_pcnt) {
		this.id_pcnt = id_pcnt;
	}

	public String getNm_exame() {
		return nm_exame;
	}

	public void setNm_exame(String nm_exame) {
		this.nm_exame = nm_exame;
	}

	public Calendar getExm_data() {
		return exm_data;
	}

	public void setExm_data(Calendar exm_data) {
		this.exm_data = exm_data;
	}

	public String getResultado() {
		return resultado;
	}

	public void setResultado(String resultado) {
		this.resultado = resultado;
	}

	public Paciente getExame_paciente() {
		return Exame_paciente;
	}

	public void setExame_paciente(Paciente exame_paciente) {
		Exame_paciente = exame_paciente;
	}
	
	
}

package model.bean;


import java.util.Date;

public class Exame {

	private int id;
	private int id_pcnt;
	private String nm_exame;
	private String exm_data;
	private String resultado;
	private Paciente examePaciente;

	public Exame() {
		super();
	}

	public Exame(int id, int id_pcnt, String nm_exame, String exm_data ,String resultado) {
		super();
		this.id = id;
		this.id_pcnt = id_pcnt;
		this.nm_exame = nm_exame;
		this.exm_data = exm_data;
		this.resultado = resultado;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getId_pcnt() {
		return id_pcnt;
	}

	public void setId_pcnt(int id_pcnt) {
		this.id_pcnt = id_pcnt;
	}

	public String getNm_exame() {
		return nm_exame;
	}

	public void setNm_exame(String nm_exame) {
		this.nm_exame = nm_exame;
	}

	public Date getExm_data() {
		return exm_data;
	}

	public void setExm_data(String string) {
		this.exm_data = string;
	}

	public String getResultado() {
		return resultado;
	}

	public void setResultado(String resultado) {
		this.resultado = resultado;
	}

	public Paciente getExamePaciente() {
		return examePaciente;
	}

	public void setExamePaciente(Paciente examePaciente) {
		this.examePaciente = examePaciente;
	}
	
	
}
